package coreconcept;

public class DataTypesAndVarisables {

	public static void main(String[] args) {
		
		/**
		 * Datatypes & Variables:
		 * primitive type -
		 * 	number-type - byte, short, int, long
		 *  decimal type - float, double
		 *  char type - char
		 *  boolean type - boolean
		 *  
		 * non-primitive type - 
		 *  String,
		 *  Arrays,
		 *  Class & Object etc,,..
		 */
		
		// Number Types
		byte b = 127;
		
		
		System.out.println(b);
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		

	}

}
