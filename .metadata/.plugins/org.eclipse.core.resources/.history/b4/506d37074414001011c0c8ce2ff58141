package L08.ExceptionHandling;

public class Main {

	public static void main(String[] args) {
		Main main = new Main();
		main.case1(10, 0);
	}
	
	public void case1(int x, int y) {
		if (y != 0) {
			System.out.println("x/y=" + x/y);
		} else {
			System.out.println("The value of y is 0, a possible exception");
		}
	}

}
