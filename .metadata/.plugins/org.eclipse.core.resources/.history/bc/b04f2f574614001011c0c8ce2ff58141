package L08.ExceptionHandling;

public class ThrowKeyword {

	public static void main(String[] args) {
		ThrowKeyword main = new ThrowKeyword();
		try {
			main.doSomething();
		} catch(Exception e) {
			e.printStackTrace();
		}

	}
	
	public void doSomething() {
		System.out.println("Do something!");
	}
	
}
